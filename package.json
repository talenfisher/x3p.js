{
  "name": "x3p.js",
  "version": "0.10.1",
  "description": "JavaScript library for working with X3P files",
  "types": "types/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint --project .",
    "test": "ENV=testing jest --colors",
    "test:render": "parcel serve tests/browser/index.html",
    "watch": "watch 'npm run test' src tests",
    "build": "parcel build src/index.ts -d dist --global X3P --no-content-hash",
    "postbuild": "node postbuild.js",
    "types": "tsc -d --emitDeclarationOnly --allowJs false --outDir types",
    "docs": "typedoc --out docs --excludePrivate --excludeProtected --excludeNotExported",
    "postdocs": "touch docs/.nojekyll",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "x3p",
    "forensics"
  ],
  "author": "Talen Fisher",
  "repository": "https://github.com/talenfisher/x3p.js",
  "license": "MIT",
  "browserslist": [
    "chrome>=60"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@types/blueimp-md5": "^2.7.0",
    "@types/file-saver": "^2.0.1",
    "@types/gl-shader": "^4.2.0",
    "@types/jest": "^24.9.1",
    "@types/jszip": "^3.1.6",
    "@types/ndarray": "^1.0.7",
    "@types/node": "^12.7.11",
    "@types/sax": "^1.2.0",
    "@types/typedarray-pool": "^1.1.2",
    "canvas": "^2.6.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "jest": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "jest-environment-jsdom-thirteen": "^1.0.1",
    "jest-transform-stub": "^1.0.0",
    "jsdom": "^13.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-raw-xml": "^1.0.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@talenfisher/canvas": "^0.3.2",
    "@talenfisher/color": "^0.1.6",
    "@talenfisher/dtype": "^0.1.2",
    "@talenfisher/multitouch-camera": "^0.2.0",
    "blueimp-md5": "^2.12.0",
    "file-saver": "^2.0.2",
    "gl-buffer": "^2.1.2",
    "gl-mat4": "^1.2.0",
    "gl-select-static": "^2.0.4",
    "gl-shader": "^4.2.1",
    "gl-texture2d": "^2.1.0",
    "gl-vao": "^1.3.0",
    "glsl-specular-beckmann": "^1.1.2",
    "jszip": "^3.2.2",
    "mouse-change": "^1.4.0",
    "ndarray": "^1.0.18",
    "ndarray-gradient": "^1.0.0",
    "typedarray-pool": "^1.1.0"
  }
}
